{
    "workbench.colorTheme": "Monokai Dimmed",
    "editor.tokenColorCustomizations": {
        "[Brackets Light Pro]": {
            "textMateRules": [
                {
                    "scope": ["variable.other.constant.js"],
                    "settings": {
                        "foreground": "#c01169" // 밝은 주황색 (상수)
                        // "fontStyle": "italic"
                    }
                },
                {
                    "scope": ["variable.other.readwrite.js"],
                    "settings": {
                        "foreground": "#d6488f" // 부드러운 갈색 (읽기/쓰기 변수)
                    }
                },
                {
                    "scope": ["storage.type.js"],
                    "settings": {
                        "foreground": "#0b4270", // 파란색 (키워드)
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "meta.object-literal.key.js",
                    "settings": {
                        "foreground": "#3c9699" // 더 어두운 색으로 강조
                    }
                },
                {
                    "scope": "entity.name.function.js",
                    "settings": {
                        "fontStyle": "italic"
                        // "fontStyle": "bold"
                    }
                },
